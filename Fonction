var numNvCarte, numRepasserCarte, numRevisionCarte : [Int] // select
var nbNvRepRev : [Int] //count
var nbTotal : Int = nbNvRepRev[0] + nbNvRepRev[1] + nbNvRepRev[2] 
//récupération des données avec la BDD


func getOrdreRevisionDuPaquet -> [Int] {
    var roue : Int = 0
    //mise en place d'une roue pour l'ordre des révisions
    var i : Int
    var ordreRevision : [Int]
    var indexNv : Int = 0
    var indexRep : Int = 0
    var indexRev : Int = 0


    while ( i < nbTotal ) {
        if ( i % 3 == 0) {
            if (indexNv <= nbNvRepRev[0]) {
                ordreRevision.append(numNvCarte[indexNv])
            }
            else {
                nbTotal ++
            }
            i ++
        }
        if ( i % 3 == 1) {
            if (indexRep <= nbNvRepRev[1]) {
                ordreRevision.append(numRepasserCarte[indexRep])
            }
            else {
                nbTotal ++
            }
            i ++
        }
        if ( i % 3 == 2) {
            if (indexRev <= nbNvRepRev[2]) {
                ordreRevision.append(numRevCarte[indexRev])
            }
            else {
                nbTotal ++
            }
            i ++
        }
    }
    return ordreRevision
}
